{
	"name": "tsed-api",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"clean": "rm -rf out",
		"clean:sdk": "rm -rf out/sdk",
		"build": "npm run barrels && tsc --project tsconfig.compile.json",
		"build:sdk": "npm run clean:sdk &&npx swagger-typescript-api -r --route-types --silent --modular --api-class-name ApiClient -p ./out/swagger.json -o ./out/sdk -n api.ts",
		"barrels": "barrelsby --config .barrelsby.json",
		"start": "npm run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
		"start:prod": "cross-env NODE_ENV=production node out/dist/index.js",
		"test:lint": "eslint '**/*.{ts,js}'",
		"test:lint:fix": "eslint '**/*.{ts,js}' --fix"
	},
	"dependencies": {
		"@tsed/ajv": "^7.14.2",
		"@tsed/cli-core": "^4.2.9",
		"@tsed/common": "^7.14.2",
		"@tsed/core": "^7.14.2",
		"@tsed/di": "^7.14.2",
		"@tsed/engines": "^7.14.2",
		"@tsed/exceptions": "^7.14.2",
		"@tsed/json-mapper": "^7.14.2",
		"@tsed/logger": "^6.3.4",
		"@tsed/logger-file": "^6.3.4",
		"@tsed/openspec": "^7.14.2",
		"@tsed/platform-cache": "^7.14.2",
		"@tsed/platform-exceptions": "^7.14.2",
		"@tsed/platform-express": "^7.14.2",
		"@tsed/platform-log-middleware": "^7.14.2",
		"@tsed/platform-middlewares": "^7.14.2",
		"@tsed/platform-params": "^7.14.2",
		"@tsed/platform-response-filter": "^7.14.2",
		"@tsed/platform-views": "^7.14.2",
		"@tsed/schema": "^7.14.2",
		"@tsed/swagger": "^7.14.2",
		"ajv": "^8.12.0",
		"barrelsby": "^2.5.1",
		"body-parser": "^1.20.2",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"dotenv-expand": "^10.0.0",
		"dotenv-flow": "^3.2.0",
		"express": "^4.18.2",
		"method-override": "^3.0.0",
		"swagger-typescript-api": "^12.0.3"
	},
	"devDependencies": {
		"@tsed/cli-plugin-eslint": "4.2.9",
		"@types/compression": "^1.7.2",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/method-override": "^0.0.32",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.14.0",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.1.2",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5"
	},
	"tsed": {
		"packageManager": "npm",
		"convention": "default",
		"architecture": "default"
	}
}
